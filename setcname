$IPpro = '102.37.101.64'
$IPsand = '102.133.185.190'

az network dns record-set a add-record --resource-group Gurosh --zone-name gurosh.com --record-set-name www --ipv4-address $IPpro
az network dns record-set a add-record --resource-group Gurosh --zone-name gurosh.com --record-set-name "@" --ipv4-address $IPpro
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name www --cname gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name agent --cname www.gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name backoffice --cname www.gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name info --cname www.gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name merchant --cname www.gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name offline --cname www.gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name wallet --cname www.gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name welcome --cname www.gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name abs --cname www.gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name api --cname www.gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name blockchain --cname www.gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name cashier --cname www.gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name emission --cname www.gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name exchange --cname www.gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name riak --cname www.gurosh.com

