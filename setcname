$IPpro = '20.74.141.248'
$IPsand = '20.74.155.8'

az network dns record-set a add-record --resource-group Gurosh --zone-name gurosh.com --record-set-name www --ipv4-address $IPpro
az network dns record-set a add-record --resource-group Gurosh --zone-name gurosh.com --record-set-name "@" --ipv4-address $IPpro
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name agent --cname gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name backoffice --cname gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name info --cname gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name merchant --cname gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name offline --cname gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name wallet --cname gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name welcome --cname gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name abs --cname gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name api --cname gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name blockchain --cname gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name cashier --cname gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name emission --cname gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name exchange --cname gurosh.com
az network dns record-set cname set-record --resource-group Gurosh --zone-name gurosh.com --record-set-name riak --cname gurosh.com

