wget https://github.com/sudaport/gurosh/blob/main/CNAME.csv
wget https://github.com/sudaport/gurosh/blob/main/SANDCNAME.csv
Import-CSV -Path "/home/tamir/CNAME.csv" | ForEach-Object { New-AzureRmDnsRecordSet -Name $_.Name -RecordType $_.RecordType -ZoneName $_.ZoneName -ResourceGroupName $_.ResourceGroupName-Ttl $_.TTL -DnsRecords (New-AzureRmDnsRecordConfig -Value $_.Value) }
Import-CSV -Path "/home/tamir/SANDCNAME.csv" | ForEach-Object { New-AzureRmDnsRecordSet -Name $_.Name -RecordType $_.RecordType -ZoneName $_.ZoneName -ResourceGroupName $_.ResourceGroupName-Ttl $_.TTL -DnsRecords (New-AzureRmDnsRecordConfig -Value $_.Value) }